[module]
enabled = true

[logging]
visual 	= true		; display and log hud messages (vlog)
error		= true		; log error messages (elog)
warning	= true		; log warning messages (wlog)
debug		= true		; log debug messages (dlog)
printf	= false		; enable GCS print function (note: this will create lots of lines)

[debug]
dev_debug	= true	; GSC debug option
sim_debug	= true	; GSC	debug option

[debug_hud]
show_top_hud							= true
show_left_hud							= true
show_right_hud						= true

keybind_shift_key					= RCONTROL	; combination of this and below will toggle huds				
keybind_toggle_top_hud 		= HOME			; keybind to cycle through available hud displays
keybind_toggle_left_hud 	= DELETE
keybind_toggle_right_hud 	= END

[cheat]
god	= true

[callbacks]
enabled = true		; allow callbacks in debug module

